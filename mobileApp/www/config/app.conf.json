[
    {
        "module": "budiApp",
        "version": "0.0.1",
        "name": "Dev",
        "proxy": {
            "_default": {
                "debug": true
            },
            "web": {
                "debug": true,
                "url": "http://localhost:3000",
                "timeout": 60000,
                "data": {}
            },
            "file": {
                "debug": true,
                "url": "data/"
            },
            "fake": {
                "debug": true
            },
            "webtransfer": {
                "debug": true,
                "url": "http://localhost:3000",
                "timeout": 60000,
                "data": {}
            }
        },
        "menu": {
            "left": {
                "content": [
                    {
                        "name": "Chat",
                        "url": "#/app/chat",
                        "icon": "fa fa-comments-o"
                    },
                    {
                        "name": "Budis",
                        "url": "#/app/budis",
                        "icon": "fa fa-users"
                    },
                    {
                        "name": "Profile",
                        "url": "#/app/profile",
                        "icon": "ion-android-user-menu"
                    },
                    {
                        "name": "Settings",
                        "url": "#/app/settings",
                        "icon": "fa fa-cogs"
                    }
                ]
            }
        },
        "auth": {
            "endpoints": {
                "_default": {
                    "policy": "web"
                },
                "facebook":{
                    "login": {
                        "url": "authentication/login",
                        "method": "POST"
                    },
                    "logout":{
                        "url": "authentication/logout",
                        "method": "POST"
                    }
                }
            }
        },
        "profile":{
            "endpoints": {
                "_default":{
                    "policy":"web"
                },
                "info":{
                    "url":"user/profile",
                    "method":"POST"
                }
            }
        }
    }
]