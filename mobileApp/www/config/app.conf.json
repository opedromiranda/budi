        "module": "budiApp",
        "version": "0.0.1",
        "name": "Dev",
        "proxy": {
            "_default": {
                "debug": true
            },
            "web": {
                "debug": true,
                "url": "http://178.62.109.12/",
                "timeout": 60000,
                "data": {}
            },
            "file": {
                "debug": true,
                "url": "data/"
